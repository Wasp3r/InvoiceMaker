<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="VeryDarkGray">#383838</Color>
    <SolidColorBrush x:Key="VeryDarkGrayBrush" Color="{StaticResource VeryDarkGray}"/>

    <Color x:Key="DarkGray">#505050</Color>
    <SolidColorBrush x:Key="DarkGrayBrush" Color="{StaticResource DarkGray}"/>

    <Color x:Key="SeaShell">#E7CFBC</Color>
    <SolidColorBrush x:Key="Background_SeaShellBrush" Color="{StaticResource SeaShell}"/>

    <Color x:Key="ChampagnePink">#FFF4EC</Color>
    <SolidColorBrush x:Key="Foreground_ChampagnePinkBrush" Color="{StaticResource ChampagnePink}"/>

    <Color x:Key="DarkPurple">#966B9D</Color>
    <SolidColorBrush x:Key="DarkPurpleBrush" Color="{StaticResource DarkPurple}"/>

    <Color x:Key="LightPurple">#CFBAF0</Color>
    <SolidColorBrush x:Key="LightPurpleBrush" Color="{StaticResource LightPurple}"/>

    <Color x:Key="Orange">#F2B880</Color>
    <SolidColorBrush x:Key="OrangeBrush" Color="{StaticResource Orange}"/>

    <Color x:Key="PaleRose">#C98686</Color>
    <SolidColorBrush x:Key="PaleRedBrush" Color="{StaticResource PaleRose}"/>

</ResourceDictionary>